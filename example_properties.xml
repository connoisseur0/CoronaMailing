<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>Application properties xml</comment>
    <entry key="hostName">mail.net</entry>
    <entry key="smtpPort">587</entry>
    <entry key="serverEmailLogin">example@dot.com</entry>
    <entry key="serverEmailPassword">pass</entry>
    <entry key="StartTLSEnabled">true</entry>
    <entry key="SSLOnConnect">false</entry>
    <entry key="receiversList">example@domain.com,Name;example2@domain.com,John</entry>
</properties>